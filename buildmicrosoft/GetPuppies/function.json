{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "puppies",
      "methods": ["get"]
    },
    {
      "type": "cosmosDB",
      "name": "allPuppies",
